<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Fejka.Repository</RootNamespace>
    <LangVersion>10</LangVersion>
    <PackageId>Fejka.Repository</PackageId>
    <Version>0.2.0</Version>
    <Authors>Torbjörn Kalin</Authors>
    <Description>An in-memory repository used for unit testing</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>http://github.com/toroso/fejka</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
